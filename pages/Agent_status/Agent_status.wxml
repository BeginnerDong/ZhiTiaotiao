
<view class="input_list1 font28 flex w690">
	<view class="input_before color8">用户姓名</view>
	<input class="input" placeholder="请输入真实的姓名" value="{{web_submitData.user_name}}"data-key="user_name" bindblur="changeBind"></input>
</view>

<view class="input_list1 font28 flex w690">
	<view class="input_before color8">身份证号</view>
	<input class="input" placeholder="请输入您的身份证号" value="{{web_submitData.cert_id}}"data-key="cert_id" bindblur="changeBind"></input>
</view>
<view class="input_list1 font28  flex w690">
	<view class="input_before color8" style="font-size: 28rpx;width: 48%;">证件到期时间</view>
	<picker mode="date" value="{{date}}" start="2000-09-01" end="2050-09-01" bindchange="bindCertEndChange" class="input">
		<view class="font28" style="font-size: 28rpx;float: right;color: #999;">{{web_certEnd?web_certEnd:'请选择'}}
			<image src="../../images/images/next-page.png" style="width: 16rpx; height: 20rpx; margin-left: 20rpx; "></image>		
		</view>
	</picker>
</view>
<view class="input_list1 font28 flex w690">
	<view class="input_before color8">手机号码</view>
	<input class="input" placeholder="请输入您的手机号码" value="{{web_submitData.user_mobile}}"data-key="user_mobile" bindblur="changeBind"></input>
</view>
<view class="input_list1 font28 flex w690">
	<view class="input_before color8">邮箱</view>
	<input class="input" placeholder="请输入您的常用邮箱" value="{{web_submitData.user_email}}"data-key="user_email" bindblur="changeBind"></input>
</view>
<view class="input_list1 font28  flex w690">
	<view class="input_before color8" style="font-size: 28rpx">职业</view>
	<picker bindchange="occupationChange" value="{{index}}" range="{{occupationData}}" range-key="name" class="input">
		<view class="font28" style="font-size: 28rpx;float: right;color: #999;">{{occupationData[web_index1].name?occupationData[web_index1].name:'请选择'}}
			<image src="../../images/images/next-page.png" style="width: 16rpx; height: 20rpx; margin-left: 20rpx; "></image>		
		</view>
	</picker>
</view>
<view class="input_list1 font28  flex w690">
	<view class="input_before color8" style="font-size: 28rpx;width:27%">用户省份</view>
	<picker bindchange="userProvinceChange" value="{{index}}" range="{{web_pArrayTwo}}" range-key="name"  class="input">
		<view class="font28" style="font-size: 28rpx;float: right;color: #999;">{{web_pArrayTwo[web_index5].name?web_pArrayTwo[web_index5].name:'请选择'}}
			<image src="../../images/images/next-page.png" style="width: 16rpx; height: 20rpx; margin-left: 20rpx; "></image>		
		</view>
	</picker>
</view>
<view class="input_list1 font28  flex w690">
	<view class="input_before color8" style="font-size: 28rpx;width:27%">用户地区</view>
	<picker bindchange="userCityChange" value="{{index}}" range="{{web_cArrayTwo}}" range-key="name"  class="input">
		<view class="font28" style="font-size: 28rpx;float: right;color: #999;">{{web_cArrayTwo[web_index6].name?web_cArrayTwo[web_index6].name:'请选择'}}
			<image src="../../images/images/next-page.png" style="width: 16rpx; height: 20rpx; margin-left: 20rpx; "></image>		
		</view>
	</picker>
</view>
