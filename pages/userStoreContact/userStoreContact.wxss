/**index.wxss**/

@import "../../wxParse/wxParse.wxss"; 
page{
	overflow:hidden;
}
.contact{
	width: 100%;
	height: 300rpx;
	margin-top: 280rpx;
}
.contact-box{
	display: inline-block;
	border:solid 3px #F8F8F8;
	width: 240rpx;
	height: 240rpx;
	margin-left: 30rpx;
}
.contact-box image{
	width: 50rpx;
	height: 65rpx;
	margin: 60rpx 95rpx 20rpx ;
}
.contact-box view{
	font-size: 28rpx;
	text-align: center;
	color: #000;
	
}
.contact-text{
	width: 100%;
	text-align: center;
	font-size: 26rpx;
	color: #646369;
	margin-top: 540rpx;
}