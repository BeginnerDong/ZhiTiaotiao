
<view class="input_list1 font28 flex w690">
	<view class="input_before color8">辅导编号：</view>
	<input placeholder="请输入辅导编号"  value="{{web_submitData.agent_no}}"  data-key="agent_no" bindblur="bindInputChange"></input>
</view>
<view class="input_list1 font28 flex  w690">
	<view class="input_before color8">本人姓名：</view>
	<input placeholder="请输入你的姓名"  value="{{web_submitData.name}}"  data-key="name" bindblur="bindInputChange"></input>
</view>
<view class="input_list1 font28 flex  w690">
	<view class="input_before color8">手机号码：</view>
	<input placeholder="请输入你的手机号码"  value="{{web_submitData.phone}}"  data-key="phone" bindblur="bindInputChange"></input>
</view>

<view class="input_list1 font28 flex  w690">
	<view class="input_before color8">登录密码：</view>
	<input placeholder="密码为6-16位字母与数字混合"  value="{{web_submitData.password}}"  data-key="password" bindblur="bindInputChange"></input>
</view>
<view class="input_list1 font28 flex  w690">
	<view class="input_before color8">常用邮箱：</view>
	<input placeholder="请输入你的常用邮箱(选填)"  value="{{web_submitData.email}}"  data-key="email" bindblur="bindInputChange"></input>
</view>
<view class="input_list1 font28 flex  w690">
	<view class="input_before color8">代理地区：</view>
  <view class="input pr" style="border:1px solid #d7d7d7">
		<picker  mode="region" bindchange="bindRegionChange" value="{{region}}" custom-item="{{customItem}}" style="width:100%">
	    <view class="picker" style="width:100%; height:48rpx">
	      {{web_submitData.address}}
        <!-- {{reginon[0]}}{{regin[1]}}{{regin[2]}} -->
	    </view>
	  </picker>
    <view class="down flex" style="width:28rpx; position: absolute; right:3%;top:24rpx;"><image src="/images/down.png"></image></view>
  </view>
</view>

<view class="input_list1 font28 w690" style="height:270rpx;display:flex;margin-top:30rpx;">
	<view class="input_before  color8" style="width:34%">上传身份证正面：</view>
	<view class="upload" wx:for="{{web_submitData.bill_img}}">
		<image src="{{item.url}}"></image>
	</view>
	<view class="upload" bindtap="upLoadPositiveId">
		<image src="/images/Upload-icon.png" style="width:58rpx;height:58rpx;"></image>
	</view>
</view>
<view class="input_list1 font28 w690" style="height:270rpx;display:flex;margin-top:30rpx;">
	<view class="input_before  color8"  style="width:34%">上传身份证反面：</view>
	<view class="upload" wx:for="{{web_submitData.bill_img}}">
		<image src="{{item.url}}"></image>
	</view>
	<view class="upload" bindtap="upLoadBillImg">
		<image src="/images/Upload-icon.png" style="width:58rpx;height:58rpx;"></image>
	</view>
</view>

<view class="input_list1 font28 w690" style="height:270rpx;display:flex;margin-top:30rpx;">
	<view class="input_before  color8"  style="width:34%">上传代理凭证：</view>
	<view class="upload" wx:for="{{web_submitData.bill_img}}">
		<image src="{{item.url}}"></image>
	</view>
	<view class="upload" bindtap="upLoadBillImg">
		<image src="/images/Upload-icon.png" style="width:58rpx;height:58rpx;"></image>
	</view>
</view>
<button class="submit_info bg2" bindtap="{{web_buttonCanClick?'submit':''}}">提交</button>