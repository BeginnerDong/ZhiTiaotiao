}


.serve-div{
	height: 70rpx;
	border-bottom: solid 1rpx #F1F1F1;
	font-size: 24rpx;
	line-height: 70rpx;
}
.serve-name{
	font-size: 28rpx;
	width: 60%;
}
.blank{
	text-align:center;
}
.blank image{
	width:214rpx;
	height:280rpx;
	margin-top:200rpx;
}

/* 添加搜索 */
.seachbox{width:100%;box-sizing:border-box;background: #fff;}
.seachbox input{ width:85%; line-height: 64rpx; height: 64rpx; padding: 0 20rpx;box-sizing: border-box;font-size: 26rpx;}
