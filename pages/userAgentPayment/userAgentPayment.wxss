page{

}
.status-top{
	height: 440rpx;
	width: 100%;
	background: #0d6fb8;
	/* margin-top: -40rpx; */
  padding-top: 50rpx;
  box-sizing:border-box;
}
.member_ranking{
	position: relative;
	top: 80rpx;
	}
.member_tlt{
	width:100%;
	height:68rpx;
	line-height:88rpx;
	text-align:center;
}
.member_cont{
	height:140rpx;
	margin-top:30rpx;
}
.member_item{
	width:33.33%;
}
.member_item .numb{
  font-size: 30rpx;
  padding-top: 10rpx;
  height: 60rpx;
}
.member_item>view{
	text-align:center;
	color:#182c4f;
}

.input_list1{
	height:70rpx;
	margin-top:10rpx;
	border-bottom:solid 1px #e0e0e0;
}
.input_list1 .input_before{
	width:22%;
