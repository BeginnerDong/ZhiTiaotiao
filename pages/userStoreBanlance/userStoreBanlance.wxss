/**index.wxss**/

@import "../../wxParse/wxParse.wxss"; 
.user_img{
	width:140rpx;
	height:140rpx;
	border-radius:50%;
	overflow:hidden;
	margin:0px auto;
}
.date{
	height:90rpx;
	border-top:solid 1px #f4f4f4;
	border-bottom:solid 1px #f4f4f4;
	display:flex;
	align-items: center;
  	justify-content: center;
  	color:#666;
}
.date_left{
	height:70rpx;
}
.date_item{
	height:90rpx;
	border-right:solid 1px #f4f4f4;
	width:50%;
	text-align:center;
	display:flex;
	align-items: center;
  	justify-content: center;
}
.date_item .text{
	height:50%;
}
.date_item image{
	width:22rpx;
	height:12rpx;
	margin-left:10rpx;
}
.team_list{
	padding:40rpx 20rpx 20rpx;
	border-bottom:solid 1px #f4f4f4;
}
.team_item{
	height:78rpx;
	display:flex;
	align-items: center;
  	justify-content: center;
}
.credit_img{
	width:14%;
}
.credit_img image{
	width:60rpx;
	height:54rpx;
}
.team_left{
	width:14%;
	text-align:left;
}
.team_right{
	width:72%;
}
.team_left image{
	width:78rpx;
	height:78rpx;
}
.teat_add{
	color:#666;
}
.credit_list{
	padding:0 34rpx;
	height:120rpx;
	border-bottom:solid 1px #e0e0e0;
}
.credit_item{
	width:100%;
	height:110rpx;
	display:flex;
	align-items:center;
	justify-content:center;
}
.credit_reson{
	color:#444;
}
.credit_left{
	width:50%;
}
.credit_right{
	width:50%;
	text-align:right;
}
.tab{
	height:110rpx;
	
}
.tab_item{
	width:50%;
	text-align:center;
	height:60rpx;
	line-height:60rpx;
	color:#04132c;
	border-bottom:solid 2px #fff;
}
.tab_item:first-child{
  border-right: 1px solid #eee
}
.tab_this{
	color:#0d6fb8;
	border-bottom:solid 2px #0d6fb8;
}
.noBorder{
	border-bottom:solid 2px #fff;
}
.blank{
	text-align:center;
}
.blank image{
	width:214rpx;
	height:280rpx;
	margin-top:200rpx;
}
.record_tlt{
	width:100%;
	/* height:50rpx; */
}
.w33{
	width:33.33%;
	text-align:center;
	height:80rpx;
	line-height:80rpx;
}
.w50{
	width:50%;
	text-align:center;
	height:80rpx;
	line-height:80rpx;
	border-right:solid 1rpx #F0F0F0 ;
}
.record_list{
	width:100%;
	height:80rpx;
	border-bottom:solid 1px #f0f0f0;
}
.record_list .w33:nth-child(2n){
	border-left:solid 1px #f0f0f0;
	border-right:solid 1px #f0f0f0;
}


.status-top{
	height: 440rpx;
	width: 100%;
	background: #0d6fb8;
}
.top-text1{
	color: #fff;
	font-size: 95rpx;
	text-align: center;
	padding-top: 100rpx;
}
.top-text2{
	color: #BDDEF1;
	font-size: 32rpx;
	text-align: center;
	margin-top: 40rpx;
}

.btn{
	display: inline-block;
	font-size: 28rpx;
	border-radius:30rpx ;
	color: #BDDEF1;
	width: 110rpx;
	height: 46rpx;
	text-align: center;
	line-height: 46rpx;
	background: #0E46A5;
	position: absolute;
	top: 45rpx;
	left:610rpx ;
}

.reword{
	width:100rpx;
	height:100rpx;
	text-align:center;
	border-radius:50%;
	overflow:hidden;
	background:#0d6fb8;
	position:fixed;
	right:0;
	bottom:34%;
	color:#fff;
}
.reword_in{
	width:68%;
	margin:0 auto;
}
.mask{
	width:100%;
	height:100%;
	position:fixed;
	left:0;
	top:0;
	background:rgba(0,0,0,0.5);
	z-index:9999;
}
.rule{
	width:90%;
	height:95%;
	position:absolute;
	left:5%;
	top:5%;
}
.rule_cont{
	width:100%;
	height:100%;
	position:relative;
}
.c{
	width:50rpx;
	height:50rpx;
	position:absolute;
	top: 920rpx;
	left: 320rpx;
}
.rule_tlt{
	width:100%;
	height:100rpx;
	line-height: 100rpx;
	text-align: center;
}
.status_number{
	font-size:50rpx;
	line-height:200rpx;
	text-align:center;
}


.content{
	text-align: justify;
	margin-bottom: 10rpx;
	line-height: 35rpx;
}
.bg1{
	border-radius:12rpx ;
}
