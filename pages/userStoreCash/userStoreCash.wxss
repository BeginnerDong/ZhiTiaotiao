/**index.wxss**/

@import "../../wxParse/wxParse.wxss"; 
page{
	background:#f2f2f2;
	overflow:hidden;
}

.cash_bank{
	width:87%;
	padding:0 6.5%;
	background:#fcfcfc;
	padding-bottom:60rpx;
}
.cash_bank input{
	border:solid 1px #e0e0e0;
	height:80rpx;
	line-height:80rpx;
	width:92%;
	padding:0 4%;
}
.cash_bank text{
	margin-right:80rpx;
}
.cash_cont{
	width:87%;
	padding:0 6.5%;
	margin:0 auto;
}
.cash_tlt{
	width:100%;
	padding:0 20rpx;
	height:140rpx;
	line-height:140rpx;
  box-sizing: border-box;
}
.input_masks{
	width:10%;
	font-size:66rpx;
}
.reword{
	width:100rpx;
	height:100rpx;
	text-align:center;
	border-radius:50%;
	overflow:hidden;
	background:#0d6fb8;
	position:fixed;
	right:0;
	bottom:20%;
	color:#fff;
}
.reword_in{
	width:68%;
	margin:0 auto;
}
.mask{
	width:100%;
	height:100%;
	position:fixed;
	left:0;
	top:0;
	background:rgba(0,0,0,0.5);
	z-index:9999;
}
.rule{
	width:90%;
	height:95%;
	position:absolute;
	left:5%;
	top:5%;
}
.rule_cont{
	width:100%;
	height:100%;
	position:relative;
}
.rule image{
	width:50rpx;
	height:50rpx;
	position:absolute;
	top: 920rpx;
	left: 320rpx;
}
.rule_tlt{
	width:100%;
	height:100rpx;
	line-height: 100rpx;
	text-align: center;
}
.status_number{
	font-size:50rpx;
	line-height:200rpx;
	text-align:center;
}


.content{
	text-align: justify;
	margin-bottom: 10rpx;
	line-height: 35rpx;
}
.bg1{
	border-radius:12rpx ;
}
.cash_input{
	width:87%;
	padding:0 6.5%;
	border-bottom:solid 1px #f0f0f0;
	height:94rpx;
}
.cash_input input{
	width:90%;
	height:80rpx;
  font-size: 54rpx;
}
.cash_can{
	width:87%;
	padding:0 6.5%;
	height:72rpx;
	line-height:72rpx;
  padding-top: 10rpx
}

.submit_info{
	width:100%;
	border-radius:0;
	margin:0 auto;
	background: #0d6fb8;
}
.bind_bank{
	height:110rpx;
	line-height:140rpx;
}
.cash_can>view{
	margin-left:40rpx;
}
.case_box{
	box-shadow:0 0 20rpx #e0e0e0;
}
.case_box{
	border-radius:20rpx;
}
.bank{
	height:140rpx;
	line-height:140rpx;
	margin-top:100rpx;
}