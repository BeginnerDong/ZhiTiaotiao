
<view class="w690 logout font24 color7">
    <view class="agent_code font28 color8"><text selectable='true'>代理NO.：{{web_userInfoData.user_no}}</text></view>
    <view class="logout_page" bindtap="loginOff">退出</view>
</view>
<view class="sort flex  bg1">
	<view class="sort_item flex" style="border-top: solid 1px #e1e1e1" bindtap="intoPath" data-path="/pages/commission/commission">
	    <view class="item_box flex">
	        <view style="width:100%;">
	            <image src="/images/store-icon5.png"></image>
	            <view class="font28 color3">佣金</view>
	        </view>
	    </view>
	</view>
	<view class="sort_item flex" style="border-top: solid 1px #e1e1e1" bindtap="intoPath" data-path="/pages/userAgentBill/userAgentBill">
	    <view class="item_box flex" style="border-left:solid 1px #e1e1e1;border-right:solid 1px #e1e1e1">
	        <view style="width:100%;">
	            <image src="/images/agency-icon3.png"></image>
	            <view class="font28 color3">账单</view>
	        </view>
	    </view>
	</view>
	<view class="sort_item flex" style="border-top: solid 1px #e1e1e1" bindtap="intoPath" data-path="/pages/userAgentService/userAgentService">
	    <view class="item_box flex">
	        <view style="width:100%;">
	            <image src="/images/agency-icon.png"></image>
	            <view class="font28 color3">代理信息</view>
	        </view>
	    </view>
	</view>
	 <view class="sort_item flex" bindtap="intoPath" data-path="/pages/userAgentStore/userAgentStore">
	    <view class="item_box flex">
	        <view style="width:100%;">
	            <image src="/images/store-icon.png"></image>
	            <view class="font28 color3">服务店铺</view>
	        </view>
	    </view>
	</view>
	<view class="sort_item flex" bindtap="intoPath" data-path="/pages/userAgentPayment/userAgentPayment">
	    <view class="item_box flex" style="border-left:solid 1px #e1e1e1;border-right:solid 1px #e1e1e1">
	        <view style="width:100%;">
	            <image src="../../images/images/user-icon6.png"></image>
	            <view class="font28 color3">辅导店铺</view>
	        </view>
	    </view>
	</view>
	<view class="sort_item flex" bindtap="intoStatus" data-path="/pages/Agent_status/Agent_status">
	    <view class="item_box flex">
	        <view style="width:100%;">
	            <image src="/images/store-icon8.png"></image>
	            <view class="font28 color3">代理开户</view>
	        </view>
	    </view>
	</view>
	<view class="sort_item flex"  style="border-right: solid 1px #e1e1e1;width: 33.5%;" bindtap="intoPath" data-path="/pages/userAgentUpload/userAgentUpload">
	    <view class="item_box flex">
	        <view style="width:100%;">
	            <image src="/images/upload.png"></image>
	            <view class="font28 color3">上传凭证</view>
	        </view>
	    </view>
	</view>
	<view class="sort_item flex" style="position: relative;" bindtap="intoPath" data-path="/pages/userStoreMsg/userStoreMsg?type=agent">
	    <view class="item_box flex" style="border-left:solid 1px #e1e1e1;box-sizing: content-box;border-right:solid 1px #e1e1e1;">
	        <view style="width:100%;">
	            <image src="/images/store-icon10.png"></image>
	            <view class="font28 color3">系统消息</view>
	        </view>
	    </view>
		<view wx:if="{{web_num}}" style="position: absolute;top:74rpx;right:90rpx;width: 20rpx;height: 20rpx;background: #DE5D1C;color: #fff;border-radius:50%;text-align: center;line-height: 20rpx;">
			
		</view>
	</view>
</view>
