/**index.wxss**/

@import "../../wxParse/wxParse.wxss"; 
page{
	overflow:hidden;
}

.login_box{
	width:95%;
	height:420rpx;
	margin-left: 10rpx;
}
.login_cont{
	position: absolute;
	left:0;
	top:0;
	height:100%;
}
.login_input{
	width:72%;
	margin:0 auto;
}
.list_input{
	width:100%;
	height:98rpx;
	line-height:98rpx;
	border-bottom:solid 1px #f0f0f0;
	overflow: hidden;
}
.list_input input{
	font-size:28rpx;
	color:#666;
	width:100%;
	height:100%;
	line-height:120rpx;
}
.list_input button{
	border: none;
}
.get_check{
	/* height:50rpx;
	text-align: center; */
	line-height:50rpx;
	background: #fff;
	color:#0d6fb8;
	font-size:28rpx;
}
button{
	border:0;
}
.submit_info{
	border-radius:10rpx;
	width:100%;
	margin:100rpx auto 0 auto;
}
.img{
	width: 100%;
}
.img image{
	width: 100%;
}

.img-phone{
	width: 30rpx; 
	height: 45rpx;
	position: relative;
	top: -550rpx; 
	left: 50rpx;
}
.img-lock{
	position: relative;
	font-size: 30rpx;
	top: -410rpx;
	left: 70rpx;
	color: #4A515A;
}
.img-locktext{
	position: relative;
	font-size: 30rpx;
	top: -350rpx;
	left: 96rpx;
	color: #4A515A;

}