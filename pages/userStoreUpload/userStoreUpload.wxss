/**index.wxss**/

@import "../../wxParse/wxParse.wxss"; 
page{
	overflow:hidden;
}
.input_list1 .input_before{
	width:30%;
}	

.input{
	width:30%;
	height:240rpx;
	line-height:240rpx;
	border:solid 1px #e0e0e0;
	background:#f2f2f2;
	text-align: center;
}
.input image{
	width:80rpx;
	height:80rpx;
	margin:0 auto;
	
}
.submit_info{
	width:100%;
	height:80rpx;
	line-height:80rpx;
	position:fixed;
	left:0;
	bottom:0;
	border-radius:0;
	margin:0 auto;
}
.upload_item{
	width:394rpx;
	height:248rpx;
	text-align: center;
	line-height:248rpx;
	background:#e0e0e0;
	margin-bottom:20rpx;
}
.upload_img{
	width:58rpx;
	height:58rpx;
}
.upload_left{
	width:20%;
	padding:0 3%;
	text-align: center;
}
.upload_right{
	width:65%;
}
.upload_item image{
	width:100%;
	height:100%;
}
.agree image{
	width:30rpx;
	height:30rpx;
	margin-right:20rpx;
}
.agree{
	margin-top:100rpx;
}
.mask{
	width:100%;
	height:100%;
	position:fixed;
	left:0;
	top:0;
	background:rgba(0,0,0,0.5);
	z-index:9999;
}
.rule{
	width:90%;
	height:80%;
	position:absolute;
	left:5%;
	top:6%;
}
.rule_cont{
	width:100%;
	height:100%;
	position:relative;
}
.rule_cont image{
	width:50rpx;
	height:50rpx;
	position:absolute;
	top:20rpx;
	right:20rpx;
}
.rule_tlt{
	width:100%;
	height:100rpx;
	line-height: 100rpx;
	text-align: center;
}