

@import "../../wxParse/wxParse.wxss"; 
page{
	overflow:hidden;
}
.reword{
	width:100rpx;
	height:100rpx;
	text-align:center;
	border-radius:50%;
	overflow:hidden;
	background:#de5d1c;
	position:fixed;
	right:0;
	bottom:20%;
	color:#fff;
}
.reword_in{
	width:68%;
	margin:0 auto;
}
.login_box{
	width:95%;
	height:420rpx;
	margin-left: 10rpx;
}
.login_cont{
	position: absolute;
	left:0;
	top:0;
	height:100%;
}
.login_input{
	width:72%;
	margin:0 auto;
}
.list_input{
	width:100%;
	height:98rpx;
	line-height:98rpx;
	border-bottom:solid 1px #f0f0f0;
}
.list_input input{
	color:#666;
	width:100%;
	height:100%;
	line-height:120rpx;
  font-size:30rpx;
  padding-left:10rpx;
  box-sizing:border-box;
  margin-top: 10rpx;
}
.list_input button{
	border: none;
}
.get_check{
	line-height:50rpx;
	background: #fff;
	color:#de5d1c;
	font-size:28rpx;
}
button{
	border:0;
}
.submit_info{
	width:100%;
	margin:100rpx auto 0 auto;
	border-radius: 40rpx;
}
.img{
	width: 100%;
}
.img image{
	width: 100%;
}

.img-phone{
	width: 30rpx; 
	height: 45rpx;
	
}


.mask{
	width:100%;
	height:100%;
	position:fixed;
	left:0;
	top:0;
	background:rgba(0,0,0,0.5);
	z-index:9999;
}
.rule{
	width:90%;
	height:95%;
	position:absolute;
	left:5%;
	top:5%;
}
.rule_cont{
	width:100%;
	height:100%;
	position:relative;
}

.rule_tlt{
	width:100%;
	height:100rpx;
	line-height: 100rpx;
	text-align: center;
}
.status_number{
	font-size:50rpx;
	line-height:200rpx;
	text-align:center;
}


.content{
	text-align: justify;
	margin-bottom: 10rpx;
	line-height: 35rpx;
}
.bg1{
	border-radius:12rpx;
}