<view class="status-top">
	<view class="w690">
	    <view class="member_ranking bg1">
	        <view class="member_cont flex">
	            <view class="member_item">
	                <view class="font26">店铺数量</view>
	                <view class="font24">100</view>
	            </view>
	            <view class="member_item">
	                <view class="font26">授权数量</view>
	                <view class="font24">60</view>
	            </view>
	            <view class="member_item">
	                <view class="font26">剩余数量</view>
	                <view class="font24">40</view>
	            </view>
	        </view>
	    </view>
	</view>
</view>
<view class="input_list1 font28 flex w690">
	<view class="input_before">辅导编号:</view>
	<input placeholder="请输入编号" value="001"></input>
</view>
<view class="input_list1 font28 flex  w690">
	<view class="input_before">本人姓名:</view>
	<input placeholder="请输入姓名"  value="刘佳"></input>
</view>
<view class="input_list1 font28 flex  w690">
	<view class="input_before">手机号码:</view>
	<input placeholder="请输入手机号码" value="15646578945"></input>
</view>
<view class="input_list1 font28 flex  w690">
	<view class="input_before">常用邮箱</view>
	<input placeholder="请输入常用邮箱"  value="15646578945@qq.com"></input>
</view>
<view class="input_list1 font28 flex  w690">
	<view class="input_before">地区</view>
	<input placeholder="请输入地址" value="464666"></input>
</view>
<view class="input_list1 font28 flex  w690" style="height: 300rpx;">
	<view class="input_before" style="width:26%; position: relative; top: -110rpx;">打款凭证</view>
	<view style="width: 200rpx; height: 240rpx; border: solid 1rpx #000;">
		<image style="width: 200rpx; height: 240rpx;"></image>
	</view>
</view>
<button style="background: #0d6fb8; width: 80%; color: #fff; font-size: 26rpx;margin-top: 80rpx; margin-bottom: 80rpx;">编辑</button>

