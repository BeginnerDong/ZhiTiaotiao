/**index.wxss**/

@import "../../wxParse/wxParse.wxss"; 

.login_box{
	width:95%;
	height:420rpx;
	margin-left: 10rpx;
}
.login_cont{
	position: absolute;
	left:0;
	top:0;
	height:100%;
}
.login_input{
	width:72%;
	margin:0 auto;
}
.list_input{
	width:100%;
	height:98rpx;
	line-height:98rpx;
	border-bottom:solid 1px #f0f0f0;
	overflow: visible;
  position: relative;
}
.list_input input{
	font-size:28rpx;
	color:#666;
	width:100%;
	height:100%;
	line-height:120rpx;
  padding-left: 10rpx;
  box-sizing:border-box;
}
.list_input button{
	border: none;
}
.get_check{
	line-height:100rpx;
	background: #fff;
	color:#0d6fb8;
	font-size:24rpx;
  position: absolute;
  right: -60rpx;
  top: 0rpx;
}
.submit_info{
	border-radius:10rpx;
	width:100%;
	margin:100rpx auto 0 auto;
}
.img{
	width: 100%;
}
.img image{
	width: 100%;
}

.register-box{
	width: 100%;
	height: 80rpx;
	font-size: 24rpx;
	box-sizing: border-box;
	margin-top: 100rpx;
}
.register-box view{
	display: inline-block;
	box-sizing: border-box;
	width: 46%;
	line-height: 80rpx;
  font-size: 30rpx;

}
.img-phone{
	width: 30rpx; 
	height: 45rpx;
	position: relative;
	top: -572rpx; 
	left: 65rpx;
}
.img-lock{
	width: 30rpx; 
	height: 35rpx;
	position: relative;
	top: -480rpx;
	left: 35rpx;
}