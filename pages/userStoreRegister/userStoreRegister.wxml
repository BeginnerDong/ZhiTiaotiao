
<view class="input_list1 font28 flex w690">
	<view class="input_before color8">店铺名称</view>
	<input placeholder="请输入店铺名称" value="{{web_submitData.name}}"  data-key="name" bindblur="bindInputChange"></input>
</view>

<view class="input_list1 font28 flex  w690">
	<view class="input_before color8">手机号码</view>
	<input placeholder="请输入手机号码" value="{{web_submitData.phone}}"  data-key="phone" bindblur="bindInputChange"></input>
</view>

<view class="input_list1 font28 flex  w690">
	<view class="input_before color8">登录密码</view>
	<input placeholder="设置密码(6位-16位)" value="{{web_submitData.password}}"  data-key="password" bindblur="bindInputChange"></input>
</view>
<view class="input_list1 font28 flex  w690">
	<view class="input_before color8">常用邮箱</view>
	<input placeholder="请输入您的邮箱" value="{{web_submitData.email}}"  data-key="email" bindblur="bindInputChange"></input>
</view>	
<view class="input_list1 font28 flex  w690">
	<view class="input_before color8">开户行</view>
	<input placeholder="请输入银行卡开户行" value="{{web_submitData.bank}}"  data-key="bank" bindblur="bindInputChange"></input>
</view>
<view class="input_list1 font28 flex  w690">
	<view class="input_before color8">银行卡号</view>
	<input placeholder="请输入银行卡号" value="{{web_submitData.bank_no}}"  data-key="bank_no" bindblur="bindInputChange"></input>
</view>
<view class="input_list1 font28 flex  w690">
	<view class="input_before color8" >行业类型:</view>
	<view class="input">
      <picker bindchange="bindPickerChange" value="{{index}}" range="{{web_typeData}}" range-key="title">
        <view class="flex picker_in">
          <view class="picker">{{web_typeData[web_index].title}}</view>
          <view class="down flex"><image src="/images/down.png"></image></view>
        </view>
      </picker>
    </view>
</view>
<view class="input_list1 font28 flex  w690">
	<view class="input_before color8" >省市县:</view>
	<view class="input">
      <view style="width:70%;" class="city">
		<picker
	    mode="region" bindchange="bindRegionChange" value="{{region}}" custom-item="{{customItem}}"
	  >
	    <view class="picker">
	      {{region[0]}}，{{region[1]}}，{{region[2]}}
	    </view>
	  </picker>
	</view>
    </view>
</view>
<view class="message flex1 w690" bindtap="chooseLocation">
	<view class="message_left font28 color2">
		详细地址
	</view>
	<view class="message_right font28 color2">
		<textarea value="{{web_submitData.address}}"  data-key="address" bindblur="bindInputChange"></textarea>
	</view>
</view>
<button class="submit_info bg2" bindtap="{{web_buttonCanClick?'submit':''}}">提交</button>



