/**index.wxss**/

@import "../../wxParse/wxParse.wxss"; 
page{
	background:#f2f2f2;
	overflow:hidden;
}

.cash_bank{
	width:87%;
	padding:0 6.5%;
	background:#fcfcfc;
	padding-bottom:60rpx;
}
.cash_bank input{
	border:solid 1px #e0e0e0;
	height:80rpx;
	line-height:80rpx;
	width:92%;
	padding:0 4%;
}
.cash_bank text{
	margin-right:80rpx;
}
.cash_cont{
	width:87%;
	padding:0 6.5%;
	margin:0 auto;
}
.cash_tlt{
	width:87%;
	padding:0 6.5%;
	height:140rpx;
	line-height:140rpx;
	text-align:center;
}
.input_masks{
	width:10%;
	font-size:60rpx;
}
.cash_input{
	width:87%;
	padding:0 6.5%;
	border-bottom:solid 1px #f0f0f0;
	height:94rpx;
}
.cash_input input{
	width:85%;
	height:80rpx;
}
.cash_can{
	width:87%;
	padding:0 6.5%;
	height:72rpx;
	line-height:72rpx;
}
.cash_can text{
	margin-left:30rpx;
}
.submit_info{
	width:100%;
	border-radius:0;
	margin:0 auto;
}
.bind_bank{
	height:110rpx;
	line-height:140rpx;
}
.cash_can>view{
	margin-left:40rpx;
}
.case_box{
	box-shadow:0 0 20rpx #e0e0e0;
}
.case_box{
	border-radius:20rpx;
}
.bank{
	height:140rpx;
	line-height:140rpx;
	margin-top:100rpx;
}
.mask{
	width:100%;
	height:100%;
	background:rgba(0,0,0,0.5);
	position:fixed;
	top:0;
	left:0;
	z-index:999;
	overflow:hidden;
}
.content{
	width:90%;
	height:80%;
	background:#fff;
	position:fixed;
	top:10%;
	left:5%;
	z-index:999;
	overflow:hidden;
}