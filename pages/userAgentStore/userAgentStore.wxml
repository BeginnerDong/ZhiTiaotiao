<view class="w690">
	<block wx:for="{{web_mainData}}" wx:key="id">
		<view class="record_list flex" bindtap="intoPath" data-path="/pages/userAgentDetail/userAgentDetail?user_no={{item.user_no}}">
			<view class="w33 color6 font24">{{item.name}}</view>
			<view class="w33 color6 font24">{{item.create_time}}</view>
		</view>
	</block>
</view>
<view class="blank" wx:if="{{web_mainData.length==0}}">
    <image src="/images/members-img.png"></image>
</view>
