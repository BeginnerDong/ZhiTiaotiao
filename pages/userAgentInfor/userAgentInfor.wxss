/**index.wxss**/

@import "../../wxParse/wxParse.wxss"; 
page{
	
}
.input_list1{
	height:90rpx;
	margin-top:30rpx;
	border-bottom:solid 1px #e0e0e0;
}
.input_list1 .input_before{
	width:22%;
	color:#04132c;
}
.input_list1 input{
	width:76%;
	padding-left:2%;
	height:100%;
	border-radius:16rpx;
}
.input{
	width:30%;
	height:240rpx;
	line-height:240rpx;
	border:solid 1px #e0e0e0;
	background:#f2f2f2;
	text-align: center;
}
.input image{
	width:80rpx;
	height:80rpx;
	margin:0 auto;
	
}
.submit_info{
	width:100%;
	height:80rpx;
	line-height:80rpx;
	position:fixed;
	left:0;
	bottom:0;
	border-radius:0;
	margin:0 auto;
}
.member_cont{
	height:140rpx;
}
.member_item{
	width:33.33%;
}
.member_item>view{
	text-align:center;
	color:#182c4f;
}
.piece_top{
	height:210rpx;
	background-color: #2a75f1;
}
.upload{
	width:30%;
	height:240rpx;
	line-height:240rpx;
	border:solid 1px #e0e0e0;
	background:#f2f2f2;
	text-align: center;
}