	            <view class="rule_cont">
	                <view class="font28 color2 rule_tlt">用户服务协议</view>
                  <view class=" content font24 color2 w690">
                    <view class="content ql-editor">
                      《用户服务协议》
                      欢迎使用知条条及服务！
                    </view>
                  </view>				
				      </view>
	        </scroll-view>
			    <image src="/images/Uploadqualification-icon.png" bindtap="rule"></image>
	    </view>
	</view>

