<block wx:for ="{{web_mainData}}" wx:key="id">

	<view class="piece_list w690 flex">
		<view class="piece_item font28 color2" style="text-align:left;">
			<view class="font24 color2">{{item.shopInfo.name}}</view>
			<view class="font24 color2">{{item.create_time}}</view>
		</view>
		<view class="piece_item font26 color5" style="text-align:right;">{{item.count}}</view>
	</view>
</block>