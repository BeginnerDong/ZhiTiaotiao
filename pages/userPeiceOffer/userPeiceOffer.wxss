/**index.wxss**/

@import "../../wxParse/wxParse.wxss"; 
page{
	background:#f2f2f2;
}
.index_product{
	flex-wrap: wrap;
}
.recommend_top{
	width:100%;
	height:212rpx;
	overflow: hidden;
}
.recommend_top image{
	width:100%;
	height:100%
}
.recommend_left{
	width:47.5%;
	border-radius: 10rpx;
	padding-bottom: 20rpx;
	height:334rpx;
	margin-top:26rpx;
}
.product_name{
	height:76rpx;
	line-height: 90rpx;
	overflow:hidden;
	width:92%;
	padding:0 4%;
}
.product_pro{
	width:86%;
	padding:0 4%;
	line-height:40rpx;
}
.product_item{
	width:33.33%;
}
.product_item image{
	width:22rpx;
	height:22rpx;
	margin-right:10rpx;
}
.register{
	width:116rpx;
	height:116rpx;
	position:fixed;
	bottom:20%;
	right:0;
	border-radius:50%;
	color:#fff;
}
.add{
	width:54rpx;
	height:54rpx;
	margin:0 auto;
	line-height:54rpx;
	text-align:center;
	font-size:54rpx;
}