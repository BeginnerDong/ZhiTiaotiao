/**index.wxss**/

@import "../../wxParse/wxParse.wxss"; 
page{
	overflow:hidden;
}
.login{
	width:100%;
	height:100vh;
	position: relative;
}

.login_box{
	width:100%;
	height:510rpx;
	margin-left: 40rpx;
}
.login_cont{
	position: absolute;
	left:0;
	top:0;
	height:100%;
}
.login_input{
	width:72%;
	margin:0 auto;
}
.list_input{
	width:100%;
	height:98rpx;
	line-height:98rpx;
	border-bottom:solid 1px #f0f0f0;
	overflow: hidden;
}
.list_input input{
	font-size:24rpx;
	color:#666;
	width:100%;
	height:100%;
	line-height:120rpx;
}
.get_check{
	width:170rpx;
	height:50rpx;
	text-align: center;
	line-height:50rpx;
	background: #f5f5f5;
	color:#666;
	font-size:24rpx;
	border:0;
}
button{
	border:0;
}
.submit_info{
	border-radius:0;
	width:100%;
	margin:100rpx auto 0 auto;
}
