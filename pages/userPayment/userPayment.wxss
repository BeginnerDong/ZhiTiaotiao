/**index.wxss**/

@import "../../wxParse/wxParse.wxss"; 
page{
	background:#f2f2f2;
	overflow:hidden;
}
.cash{	
	padding-bottom:50rpx;
}
.cash_bank{
	width:87%;
	padding:0 6.5%;
	background:#fcfcfc;
	padding-bottom:60rpx;
}
.cash_bank input{
	border:solid 1px #e0e0e0;
	height:80rpx;
	line-height:80rpx;
	width:92%;
	padding:0 4%;
}
.cash_bank text{
	margin-right:80rpx;
}
.cash_cont{
	width:87%;
	padding:0 6.5%;
	margin:0 auto;
}
.cash_tlt{
	width:87%;
	padding:0 6.5%;
	height:140rpx;
	line-height:140rpx;
	text-align:center;
}
.input_masks{
	width:10%;
	font-size:60rpx;
}
.cash_input{
	width:87%;
	padding:0 6.5%;
	border-bottom:solid 1px #f0f0f0;
	height:94rpx;
}
.cash_input input{
	width:85%;
	height:80rpx;
}
.cash_can{
	width:87%;
	padding:0 6.5%;
	height:72rpx;
	line-height:72rpx;
}
.cash_can text{
	margin-left:30rpx;
}
.submit_info{
	width:100%;
	border-radius:0;
	margin:0 auto;
}
.bind_bank{
	height:110rpx;
	line-height:140rpx;
}
.cash_can>view{
	margin-left:40rpx;
}
.case_box{
	box-shadow:0 0 20rpx #e0e0e0;
}
.case_box{
	margin-top:100rpx;
	border-radius:20rpx;
	padding-bottom:30rpx;
}
.payment_item{
	width:100%;
	height:90rpx;
}
.payment_item image{
	width:30rpx;
	height:30rpx;
	margin-right:20rpx;
}
.mask{
	width:100%;
	height:100%;
	background:rgba(0,0,0,0.5);
	position:fixed;
	overflow: hidden;
	left:0;
	top:0;
}
.confirm{
	width:70%;
	height:336rpx;
	border-radius:20rpx;
	position:absolute;
	left:15%;
	top:30%;
}
.confirm_tlt{
	width:100%;
	height:120rpx;
	line-height:120rpx;
	font-size:28rpx;
	color:#222;
	text-align:center;
}
.confirm_notice{
	height:120rpx;
	text-align:center;
}
.confirm_btn{
	width:100%;
	height:96rpx;
	border-top:solid 1px #e0e0e0;
}
.confirm_item{
	width:50%;
	height:96rpx;
	line-height:96rpx;
	text-align:center;
}
.get_peice{
	position:absolute;
	left:0;
	width:100%;
	top:30%;
	text-align:center;
}
.get_peice image{
	width:446rpx;
	height:360rpx;
	margin:0 auto;
}
.close{
	position:absolute;
	left:0;
	width:100%;
	bottom:10%;
	text-align:center;
}
.close image{
	width:80rpx;
	height:80rpx;
	margin:0 auto;
}